server {
    listen      *:443 ssl;
    server_name training.galaxyproject.org;

    access_log  /var/log/nginx/training.galaxyproject.org_access.log;
    error_log   /var/log/nginx/training.galaxyproject.org_error.log;

    #client_max_body_size 8M;

    #root        /srv/nginx/training.galaxyproject.org/root;

    #error_page  404      /error.html;
    #error_page  403 =404 /error.html;

    location /archive {
        proxy_cache            s3_cache;
        proxy_http_version     1.1;
        proxy_set_header       Connection "";
        proxy_set_header       Authorization '';
        proxy_set_header       Host galaxy-training.s3-website.us-east-1.amazonaws.com;
        proxy_hide_header      x-amz-id-2;
        proxy_hide_header      x-amz-request-id;
        proxy_hide_header      x-amz-meta-server-side-encryption;
        proxy_hide_header      x-amz-server-side-encryption;
        proxy_hide_header      Set-Cookie;
        proxy_ignore_headers   Set-Cookie;
        proxy_cache_revalidate on;
        #proxy_intercept_errors on;
        #proxy_cache_use_stale  error timeout updating http_500 http_502 http_503 http_504;
        proxy_cache_lock       on;
        proxy_cache_valid      200 304 60m;
        add_header             Cache-Control max-age=31536000;
        add_header             X-Cache-Status $upstream_cache_status;
        proxy_pass             http://galaxy-training.s3-website.us-east-1.amazonaws.com/archive;

        proxy_set_header       Content-Security-Policy-Report-Only "default-src 'self unsafe-inline'; script-src 'report-sample' 'self unsafe-inline' https://browser.sentry-cdn.com/7.52.1/bundle.tracing.min.js https://cdnjs.cloudflare.com/ajax/libs/mathjax/2.7.1/config/TeX-AMS-MML_HTMLorMML.js https://kit.fontawesome.com/67b3f98409.js https://platform.twitter.com/widgets.js https://plausible.galaxyproject.eu/js/plausible.js https://sidecar.gitter.im/dist/sidecar.v1.js; style-src 'report-sample' 'self unsafe-inline' https://fonts.googleapis.com; object-src 'none'; base-uri 'self'; connect-src 'self' https://ka-f.fontawesome.com https://plausible.galaxyproject.eu https://sentry.galaxyproject.org; font-src 'self' https://cdnjs.cloudflare.com https://fonts.gstatic.com https://ka-f.fontawesome.com; frame-src 'self' https://platform.twitter.com https://plausible.galaxyproject.eu https://syndication.twitter.com https://www.youtube-nocookie.com; img-src 'all'; manifest-src 'self'; media-src 'self'; report-uri https://sentry.galaxyproject.org/api/10/security/?sentry_key=45e0ec6e4373462b92969505df37cf40; worker-src 'none';";
    }
    
    location /videos/topic {
        proxy_cache            s3_cache;
        proxy_http_version     1.1;
        proxy_set_header       Connection "";
        proxy_set_header       Authorization '';
        proxy_set_header       Host galaxy-training.s3-website.us-east-1.amazonaws.com;
        proxy_hide_header      x-amz-id-2;
        proxy_hide_header      x-amz-request-id;
        proxy_hide_header      x-amz-meta-server-side-encryption;
        proxy_hide_header      x-amz-server-side-encryption;
        proxy_hide_header      Set-Cookie;
        proxy_ignore_headers   Set-Cookie;
        proxy_set_header       Range $http_range;
        proxy_set_header       If-Range $http_if_range;
        proxy_cache_revalidate on;
        #proxy_intercept_errors on;
        #proxy_cache_use_stale  error timeout updating http_500 http_502 http_503 http_504;
        proxy_cache_lock       on;
        proxy_cache_valid      200 304 60m;
        add_header             Cache-Control max-age=31536000;
        add_header             X-Cache-Status $upstream_cache_status;
        proxy_pass             http://galaxy-training.s3-website.us-east-1.amazonaws.com/videos/topic;

        proxy_set_header       Content-Security-Policy-Report-Only "default-src 'self unsafe-inline'; script-src 'report-sample' 'self unsafe-inline' https://browser.sentry-cdn.com/7.52.1/bundle.tracing.min.js https://cdnjs.cloudflare.com/ajax/libs/mathjax/2.7.1/config/TeX-AMS-MML_HTMLorMML.js https://kit.fontawesome.com/67b3f98409.js https://platform.twitter.com/widgets.js https://plausible.galaxyproject.eu/js/plausible.js https://sidecar.gitter.im/dist/sidecar.v1.js; style-src 'report-sample' 'self unsafe-inline' https://fonts.googleapis.com; object-src 'none'; base-uri 'self'; connect-src 'self' https://ka-f.fontawesome.com https://plausible.galaxyproject.eu https://sentry.galaxyproject.org; font-src 'self' https://cdnjs.cloudflare.com https://fonts.gstatic.com https://ka-f.fontawesome.com; frame-src 'self' https://platform.twitter.com https://plausible.galaxyproject.eu https://syndication.twitter.com https://www.youtube-nocookie.com; img-src 'all'; manifest-src 'self'; media-src 'self'; report-uri https://sentry.galaxyproject.org/api/10/security/?sentry_key=45e0ec6e4373462b92969505df37cf40; worker-src 'none';";
    }

    location /training-material/ {
        proxy_http_version     1.1;
        proxy_set_header       Host galaxyproject.github.io;
        proxy_pass             https://galaxyproject.github.io/training-material/;

        proxy_set_header       Content-Security-Policy-Report-Only "default-src 'self unsafe-inline'; script-src 'report-sample' 'self unsafe-inline' https://browser.sentry-cdn.com/7.52.1/bundle.tracing.min.js https://cdnjs.cloudflare.com/ajax/libs/mathjax/2.7.1/config/TeX-AMS-MML_HTMLorMML.js https://kit.fontawesome.com/67b3f98409.js https://platform.twitter.com/widgets.js https://plausible.galaxyproject.eu/js/plausible.js https://sidecar.gitter.im/dist/sidecar.v1.js; style-src 'report-sample' 'self unsafe-inline' https://fonts.googleapis.com; object-src 'none'; base-uri 'self'; connect-src 'self' https://ka-f.fontawesome.com https://plausible.galaxyproject.eu https://sentry.galaxyproject.org; font-src 'self' https://cdnjs.cloudflare.com https://fonts.gstatic.com https://ka-f.fontawesome.com; frame-src 'self' https://platform.twitter.com https://plausible.galaxyproject.eu https://syndication.twitter.com https://www.youtube-nocookie.com; img-src 'all'; manifest-src 'self'; media-src 'self'; report-uri https://sentry.galaxyproject.org/api/10/security/?sentry_key=45e0ec6e4373462b92969505df37cf40; worker-src 'none';";
    }

    location ~ ^/training-material/api/ga4gh/(.*) {
        proxy_http_version     1.1;
        proxy_set_header       Host galaxyproject.github.io;
        proxy_pass             https://galaxyproject.github.io/training-material/api/ga4gh/$1.json;
    }

    location / {
        proxy_cache            s3_cache;
        proxy_http_version     1.1;
        proxy_set_header       Host galaxyproject.github.io;
        proxy_pass             https://galaxyproject.github.io/training-material/;

        proxy_set_header       Content-Security-Policy-Report-Only "default-src 'self unsafe-inline'; script-src 'report-sample' 'self unsafe-inline' https://browser.sentry-cdn.com/7.52.1/bundle.tracing.min.js https://cdnjs.cloudflare.com/ajax/libs/mathjax/2.7.1/config/TeX-AMS-MML_HTMLorMML.js https://kit.fontawesome.com/67b3f98409.js https://platform.twitter.com/widgets.js https://plausible.galaxyproject.eu/js/plausible.js https://sidecar.gitter.im/dist/sidecar.v1.js; style-src 'report-sample' 'self unsafe-inline' https://fonts.googleapis.com; object-src 'none'; base-uri 'self'; connect-src 'self' https://ka-f.fontawesome.com https://plausible.galaxyproject.eu https://sentry.galaxyproject.org; font-src 'self' https://cdnjs.cloudflare.com https://fonts.gstatic.com https://ka-f.fontawesome.com; frame-src 'self' https://platform.twitter.com https://plausible.galaxyproject.eu https://syndication.twitter.com https://www.youtube-nocookie.com; img-src 'all'; manifest-src 'self'; media-src 'self'; report-uri https://sentry.galaxyproject.org/api/10/security/?sentry_key=45e0ec6e4373462b92969505df37cf40; worker-src 'none';";
    }

    #return 302 https://galaxyproject.github.io/training-material$request_uri;
