server {
    listen      *:443 ssl default_server;
    server_name rochefort.galaxyproject.org;

    access_log  /var/log/nginx/rochefort.galaxyproject.org_access.log;
    error_log   /var/log/nginx/rochefort.galaxyproject.org_error.log;

    root        /srv/nginx/rochefort.galaxyproject.org/root;

    location / {
        return 302 http://galaxyproject.org;
    }
}

