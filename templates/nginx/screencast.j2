server {
    listen      *:443 ssl;
    server_name screencast.g2.bx.psu.edu screencast.galaxyproject.org;

    access_log  /var/log/nginx/screencast.galaxyproject.org_access.log;
    error_log   /var/log/nginx/screencast.galaxyproject.org_error.log;

    root        /srv/nginx/screencast.galaxyproject.org/root;

    return      302 https://galaxyproject.org/learn/screencasts;
}

