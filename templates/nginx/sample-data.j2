server {
    listen      *:443 ssl;
    server_name sample-data.galaxyproject.org;

    access_log  /var/log/nginx/sample-data.galaxyproject.org_access.log;
    error_log   /var/log/nginx/sample-data.galaxyproject.org_error.log;

    root        /srv/nginx/sample-data.galaxyproject.org/root;

    location / {
        autoindex on;
    }
}

