# LiteLLM Environment Variables
{% if litellm_database_type == 'postgresql' %}
DATABASE_URL=postgresql://{{ litellm_database_user }}:{{ litellm_database_password }}@{{ litellm_database_host }}:{{ litellm_database_port }}/{{ litellm_database_name }}
{% elif litellm_database_type == 'sqlite' %}
DATABASE_URL=sqlite:///{{ litellm_sqlite_path }}
{% endif %}
LITELLM_MASTER_KEY={{ litellm_master_key }}
LITELLM_LOG=DEBUG
